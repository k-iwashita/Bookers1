<h1>Editing Book</h1>

<%= render "error_messages" %>

<%= form_for(@book) do |f| %>
    <%= f.label :title%>
    <%= f.text_field :title %>
    <%= f.label :body%>
    <%= f.text_area :body %>
    <%= f.submit 'Update Book' %>
<% end %>

<%= link_to "show", book_path(@book) %>  |
<%= link_to "Back", books_path %>
